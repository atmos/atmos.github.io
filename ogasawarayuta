<script type="text/javascript">

$(".tabs").on("click", "li", function() {
		$(this).siblings('.checked').removeClass('checked');
		$(this).addClass('checked');
		var id = $(this).index();
		$(".fluid iframe").removeClass('show');
		$(".fluid iframe").eq(id).addClass('show');
		});
</script>
